{
	"name": "ejjy",
	"version": "0.1.0",
	"private": true,
	"homepage": "./",
	"main": "./electron/main.js",
	"scripts": {
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"deploy": "python3 deploy.py",
		"start:online": "env-cmd -f .env.online react-scripts start",
		"start:local": "env-cmd -f .env.local react-scripts start",
		"build:online": "env-cmd -f .env.online react-scripts build",
		"build:local": "env-cmd -f .env.local react-scripts build",
		"start:electron": "PORT=3001 env-cmd -f .env.local react-scripts start",
		"dev:electron": "concurrently \"npm start:electron\" \"wait-on http://localhost:3001 && electron .\"",
		"electron": "wait-on http://localhost:3001 && electron .",
		"ebuild": "npm run build && node_modules/.bin/build",
		"pack": "electron-packager ./ EJJY-Cashiering --platform=win32"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"antd": "^4.6.2",
		"axios": "^0.20.0",
		"classnames": "^2.2.6",
		"connected-react-router": "^6.8.0",
		"craco-less": "^1.17.0",
		"dayjs": "^1.9.4",
		"electron-is-dev": "^1.2.0",
		"formik": "^2.1.5",
		"lodash": "^4.17.20",
		"moment": "^2.29.1",
		"rc-resize-observer": "^0.2.3",
		"react": "^16.13.1",
		"react-barcode-reader": "0.0.2",
		"react-beforeunload": "^2.4.0",
		"react-detect-offline": "^2.4.1",
		"react-dom": "^16.13.1",
		"react-keyboard-event-handler": "^1.5.4",
		"react-redux": "^7.2.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.3",
		"react-window": "^1.8.5",
		"redux-actions": "^2.6.5",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.3",
		"reselect": "^4.0.0",
		"sweetalert": "^2.1.2",
		"typescript": "^3.7.5",
		"yup": "^0.29.3"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/classnames": "^2.2.10",
		"@types/jest": "^24.9.1",
		"@types/lodash": "^4.14.161",
		"@types/node": "^12.12.55",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@types/react-redux": "^7.1.9",
		"@types/react-router-dom": "^5.1.5",
		"@types/redux-actions": "^2.6.1",
		"@types/yup": "^0.29.6",
		"concurrently": "^5.3.0",
		"copy-webpack-plugin": "^6.1.0",
		"electron": "^11.2.1",
		"electron-builder": "^22.9.1",
		"electron-packager": "^15.2.0",
		"env-cmd": "^10.1.0",
		"node-sass": "^4.14.1",
