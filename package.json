{
	"name": "ejjy-inventory-app",
	"productName": "EJJY Inventory",
	"version": "0.1.282",
	"private": true,
	"homepage": "./",
	"main": "./public/electron.js",
	"scripts": {
		"lint": "cd src && eslint . --ext .tsx --ext .ts",
		"lint:fix": "cd src && eslint . --ext .tsx --ext .ts --fix",
		"prettify": "prettier --write ./src",
		"deploy": "npm version patch --no-git-tag-version && python deploy.py",
		"start:online": "env-cmd -f .env.online react-scripts start",
		"start:local": "env-cmd -f .env.local react-scripts start",
		"start:electron": "concurrently \"PORT=3010 env-cmd -f .env.local react-scripts start\" \"wait-on http://localhost:3010 && electron .\"",
		"server:bm": "cd ./api && poetry run python manage.py runserver 8000",
		"server:bm:branch": "concurrently \"npm run server:bm\" \"cd ../ejjy-app-cashiering/api && poetry run python manage.py runserver 8005\"",
		"start:bm": "concurrently \"npm run server:bm:branch\" \"npm run start:local\"",
		"server:om": "cd ../ejjy-api-officemanager && poetry run python manage.py runserver 8001",
		"server:om:branch": "concurrently \"npm run server:bm\" \"npm run server:om\"",
		"start:om": "concurrently \"npm run server:om:branch\" \"npm run start:local\"",
		"server:all": "concurrently \"npm run server:bm:branch\" \"npm run server:om\"",
		"electron": "wait-on http://localhost:3010 && electron .",
		"postinstall": "electron-builder install-app-deps",
		"build:online": "env-cmd -f .env.online react-scripts build",
		"build:local": "env-cmd -f .env.local react-scripts build",
		"build:electron": "electron-builder -w --x64 -p always",
		"publish": "npm version patch --no-git-tag-version && npm run build:local && npm run build:electron"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"antd": "4.19.1",
		"antd-css-utilities": "^1.0.3",
		"array-move": "^4.0.0",
		"axios": "^0.21.1",
		"classnames": "^2.2.6",
		"connected-react-router": "^6.8.0",
		"dayjs": "^1.10.4",
		"electron-is-dev": "^2.0.0",
		"electron-log": "^4.4.1",
		"electron-store": "^8.1.0",
		"electron-updater": "4.3.9",
		"focus-formik-error": "^1.1.0",
		"formik": "^2.2.6",
		"history": "^4.10.1",
		"i": "^0.3.7",
		"jsbarcode": "^3.11.5",
		"jspdf": "^2.3.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"npm": "^8.13.2",
		"qrcode": "^1.5.1",
		"query-string": "^7.0.1",
		"qz-tray": "^2.1.2",
		"rc-resize-observer": "^0.2.3",
		"react": "^16.13.1",
		"react-barcode-reader": "0.0.2",
		"react-beforeunload": "^2.4.0",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-keyboard-event-handler": "^1.5.4",
		"react-markdown": "^8.0.3",
		"react-query": "^3.34.15",
		"react-redux": "^7.2.2",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.3",
		"react-sortable-hoc": "^2.0.0",
		"react-window": "^1.8.5",
		"redux": "^4.1.0",
		"redux-actions": "^2.6.5",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.3",
		"remark-gfm": "^3.0.1",
		"reselect": "^4.0.0",
		"sweetalert": "^2.1.2",
		"tree-kill": "^1.2.2",
		"typescript": "^3.7.5",
		"use-debounce": "^7.0.1",
		"yup": "^0.29.3",
		"zustand": "^4.1.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-react": "^7.14.5",
		"@types/classnames": "^2.2.10",
		"@types/history": "^5.0.0",
		"@types/lodash": "^4.14.161",
		"@types/node": "^12.12.55",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@types/react-redux": "^7.1.9",
		"@types/react-router-dom": "^5.1.5",
		"@types/redux-actions": "^2.6.1",
		"@types/yup": "^0.29.6",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"babel-plugin-import": "^1.13.3",
		"concurrently": "^5.3.0",
		"electron": "^17.4.7",
		"electron-builder": "^22.14.5",
		"env-cmd": "^10.1.0",
		"eslint": "^7.15.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^7.0.0",
		"eslint-import-resolver-typescript": "^2.7.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.0",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4",
		"prettier": "^2.2.1",
		"sass": "^1.49.9",
		"wait-on": "^5.2.1"
	},
	"build": {
		"appId": "com.ejjy.inventory.app",
		"extraResources": [
			"api/**",
			"gcs-service-key.json",
			"!api/.venv",
			"!api/db.sqlite3",
			"!api/backend/.env"
		],
		"publish": [
			{
				"provider": "github",
				"owner": "EJJY-Devs",
				"repo": "ejjy-app",
				"releaseType": "release"
			}
		]
	}
}
