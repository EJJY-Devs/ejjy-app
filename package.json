{
	"name": "ejjy",
	"version": "0.1.26",
	"private": true,
	"homepage": "./",
	"main": "./electron/main.js",
	"scripts": {
		"lint": "cd src && eslint . --ext .tsx --ext .ts",
		"lint:fix": "cd src && eslint . --ext .tsx --ext .ts --fix",
		"prettify": "prettier --write ./src",
		"deploy": "npm version patch --no-git-tag-version && python3 deploy.py",
		"start:online": "PORT=3001 env-cmd -f .env.online react-scripts start",
		"start:local": "env-cmd -f .env.local react-scripts start",
		"build:online": "env-cmd -f .env.online react-scripts build",
		"build:local": "env-cmd -f .env.local react-scripts build",
		"start:electron": "PORT=3005 env-cmd -f .env.local react-scripts start",
		"dev:electron": "concurrently \"npm run start:electron\" \"wait-on http://localhost:3005 && electron .\"",
		"electron": "wait-on http://localhost:3005 && electron .",
		"pack:build": "npm run build:local && electron-packager . EJJY-Inventory --no-prune --platform=win32 --ignore=/.vscode --ignore=/deployment --ignore=/node_modules --ignore=/public --ignore=/src --ignore=/.prettierignore --ignore=/.prettierrc --ignore=/.config-overrides.js --ignore=/package-lock.json --ignore=/README.md --ignore=/tsconfig.json --ignore=/yarn.lock --ignore=/electron-dev --overwrite",
		"pack:zip": "7z a -tzip EJJY-Inventory-win32-ia32.zip EJJY-Inventory-win32-ia32",
		"pack": "npm run pack:build && npm run pack:zip"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"antd": "^4.16.6",
		"axios": "^0.21.1",
		"classnames": "^2.2.6",
		"connected-react-router": "^6.8.0",
		"craco-less": "^1.17.0",
		"dayjs": "^1.10.4",
		"electron-is-dev": "^1.2.0",
		"formik": "^2.2.6",
		"history": "^4.10.1",
		"jspdf": "^2.3.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"qz-tray": "^2.1.2",
		"rc-resize-observer": "^0.2.3",
		"react": "^16.13.1",
		"react-barcode-reader": "0.0.2",
		"react-beforeunload": "^2.4.0",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-keyboard-event-handler": "^1.5.4",
		"react-redux": "^7.2.2",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.3",
		"react-window": "^1.8.5",
		"redux": "^4.1.0",
		"redux-actions": "^2.6.5",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.3",
		"reselect": "^4.0.0",
		"sweetalert": "^2.1.2",
		"typescript": "^3.7.5",
		"yup": "^0.29.3"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/classnames": "^2.2.10",
		"@types/jest": "^24.9.1",
		"@types/lodash": "^4.14.161",
		"@types/node": "^12.12.55",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@types/react-redux": "^7.1.9",
		"@types/react-router-dom": "^5.1.5",
		"@types/redux-actions": "^2.6.1",
		"@types/yup": "^0.29.6",
		"@typescript-eslint/eslint-plugin": "^4.28.1",
		"@typescript-eslint/parser": "^4.28.1",
		"concurrently": "^5.3.0",
		"copy-webpack-plugin": "^6.1.0",
		"electron": "^11.2.1",
		"electron-builder": "^22.9.1",
		"electron-packager": "^15.2.0",
		"env-cmd": "^10.1.0",
		"eslint": "^7.29.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"node-sass": "^4.14.1",
		"prettier": "^2.3.2",
		"react-app-rewired": "^2.1.6",
		"wait-on": "^5.2.1"
	}
}
